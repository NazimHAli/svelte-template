        "@types/compression": "^1.7.2",
        "@types/cssnano": "^4.0.1",
        "@types/express": "^4.17.13",
        "@types/node": "^16.11.11",
        "@types/postcss-import": "^14.0.0",
        "@types/tailwindcss": "^2.2.4",
        "animate.css": "^4.1.1",
        "autoprefixer": "^10.4.0",
        "cssnano": "^5.0.12",
        "cypress": "^9.1.1",
        "express-rate-limit": "^5.5.1",
        "postcss": "^8.4.4",
        "postcss-import": "^14.0.2",
        "postcss-nested": "^5.0.6",
        "postcss-sass": "^0.5.0",
        "prettier": "^2.5.1",
        "prettier-plugin-svelte": "^2.5.0",
        "sass": "^1.44.0",
