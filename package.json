        "svelte": "^3.44.2",
        "svelte-check": "^2.2.10",
        "svelte-preprocess": "^4.7.2",
        "tailwindcss": "^2.2.19",
        "ts-node": "^10.4.0",
        "tslib": "^2.3.1",
        "typescript": "^4.5.2",
        "vite": "^2.9.13",
        "wait-on": "^6.0.0"
